# rota(başlangıç-bitiş) için süreyi belirledim.
rotasuresi <- function(konum_a, konum_b) { # kunum_a ve konum_b başlangıç ve varış olarak değerlendirdim.
return(runif(1, 10, 60)) # rota süresi soruda olmadığı için return ile rastgele belirledim.
}
rotailk <- rotasuresi(konum_ailk, konum_ailk) #rota süresi ilk alternatif yol için
rotaiki <- rotasuresi(konum_bilk, konum_bilk) #rota süresi ikinci alternatif yol için
rota_farki <- abs(rotailk - rotaiki) #rota farkı hesapladım karşılaştırma için
if(rota_farki > 15) {
if(rotailk < rotaiki) {
print("En kısa rota 1. Rota")
} else {
print("En kısa rota 2. Rota")
}
} else {
print("Her iki rotayı da seçmeniz sizin için uygundur.")
}
